{
    "categories": [
        {
            "title": "## ğŸš€ Features",
            "labels": ["feat"]
        },
        {
            "title": "## ğŸ› Fixes",
            "labels": ["fix"]
        },
        {
            "title": "## ğŸ§ª Tests",
            "labels": ["test"]
        },
        {
            "title": "## ğŸ’¬ Other",
            "labels": ["chore", "ci"]
        },
        {
            "title": "## ğŸ“¦ Uncategorized",
            "labels": []
        }
    ],
    "pr_template": "${{TITLE}}",
    "label_extractor": [
        {
            "pattern": "(.+?)(\\(.+\\))?: (.+)",
            "target": "$1"
        }
    ],
    "transformers": [
        {
            "pattern": "(.+?)\\((.+)\\): (.+)",
            "target": "- $2: $3"
        },
        {
            "pattern": "^([^-()]+?): (.+)",
            "target": "- $2"
        }
    ],
    "max_pull_requests": 1000,
    "max_back_track_time_days": 1000
}
